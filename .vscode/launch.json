    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/bin/www",
            "env": {
              "NODE_ENV":"${env:NODE_ENV}",
              "RELAY_URL":"${env:RELAY_URL}",
              "PRIVATE_KEY": "${env:PRIVATE_KEY}",
              "PUBLIC_KEY": "${env:PUBLIC_KEY}",
              "REDIS_HOST":"${env:REDIS_HOST}",
              "REDIS_PORT":"${env:REDIS_PORT}",
              "DB_HOST":"${env:DB_HOST}",
              "DB_PORT":"${env:DB_PORT}",
              "DB_NAME":"${env:DB_NAME}",
              "DB_USER":"${env:DB_USER}",
              "DB_PASS":"${env:DB_PASS}",
            }
        }
    ]
}